{
  "name": "asf-navbar",
  "version": "0.2.13",
  "author": "walker2398",
  "contributors": [
    {
      "name": "harass",
      "email": "suidar@foxmail.com"
    }
  ],
  "main": "src/entries/main.ts",
  "types": "src/entries/main.ts",
  "scripts": {
    "serve": "set ASF_VUE=1 && vue-cli-service serve",
    "serve:vue": "ASF_VUE=1 vue-cli-service serve",
    "serve:react": "ASF_REACT=1 vue-cli-service serve",
    "build": "set ASF_VUE=1 && set ASF_REACT=1 && set NODE_ENV=production && vue-cli-service build",
    "inspect": "ASF_REACT=1 ASF_VUE=1 NODE_ENV=production vue inspect",
    "lint": "vue-cli-service lint",
    "prepare": "husky install",
    "clean": "rm -rf dist/*"
  },
  "dependencies": {
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "react": "^17.0.2",
    "vue": "^2.6.11",
    "vue-avatar": "^2.3.3"
  },
  "devDependencies": {
    "@types/react": "^17.0.19",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^7.0.1",
    "less-loader": "^7.3.0",
    "react-dom": "^17.0.2",
    "ts-loader": "^8.2.0",
    "typescript": "^4.4.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "description": "asf网站导航栏",
  "repository": {
    "type": "git",
    "url": "git+https://gitee.com/asoul-fan/asf-navbar-vue.git"
  }
}
